{
  "name": "app",
  "version": "1.0.50",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "bun run --watch src/index.ts",
    "db:push": "bunx prisma db push",
    "db:studio": "bunx prisma studio",
    "prepare": "husky",
    "commit": "git-cz",
    "lint": "bunx biome lint ./src",
