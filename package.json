    "format:fix": "bunx biome format --write ./src"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.1.1",
    "@elysiajs/jwt": "^1.1.1",
    "@elysiajs/stream": "^1.1.0",
    "@elysiajs/swagger": "^1.1.5",
    "@prisma/client": "5.20.0",
    "elysia": "latest",
    "ioredis": "^5.4.1",
    "minio": "^8.0.2",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@types/mime-types": "^2.1.4",
    "bun-types": "latest",
    "cz-conventional-changelog": "^3.3.0",
    "cz-git": "^1.10.1",
    "eslint": "^9.14.0",
    "globals": "^15.11.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "mime-types": "^2.1.35",
    "prisma": "^5.21.1",
    "typescript-eslint": "^8.12.2"
