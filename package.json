    "elysia": "latest",
    "ioredis": "^5.4.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@commitlint/cli": "^19.4.1",
    "@commitlint/config-conventional": "^19.4.1",
    "@eslint/js": "^9.10.0",
    "bun-types": "latest",
    "cz-conventional-changelog": "^3.3.0",
    "cz-git": "^1.9.4",
    "eslint": "^9.10.0",
    "globals": "^15.9.0",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.10",
    "prisma": "^5.19.1",
    "typescript-eslint": "^8.4.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
